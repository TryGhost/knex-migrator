#!/usr/bin/env node
'use strict';

const logging = require('../lib/logging');
const command = require('../lib/command');

command(
    program => program.option('--skip <item>').option('--only <item>'),
    (knexMigrator, args) => knexMigrator.init({
        skip: args.skip,
        only: args.only
    }).then(() => {
        logging.info('Finished database init!');
    })
);
