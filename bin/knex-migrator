#!/usr/bin/env node

var program = require('commander'),
    pkg = require('../package.json');

program
    .version(pkg.version, '-v, --version')
    .command('init [config]', 'init db').alias('i')
    .command('migrate [config]', 'migrate db').alias('m')
    .command('reset', 'reset db').alias('r')
    .parse(process.argv);